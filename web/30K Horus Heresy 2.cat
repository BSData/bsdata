                  <entries>
                    <entry id="5b0017fe-1d26-5b91-6a54-0a25c46b1818" name="Erratic Psychic Power" points="0.0" categoryId="(No Category)" type="upgrade" minSelections="0" maxSelections="1" minPoints="0.0" maxPoints="-1.0" minInRoster="0" maxInRoster="-1" collective="false" page="0">
                      <entries/>
                      <entryGroups/>
